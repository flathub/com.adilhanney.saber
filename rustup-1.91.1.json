{
    "name": "rustup",
    "buildsystem": "simple",
    "build-options": {
        "env": {
            "CARGO_HOME": "/var/lib/rustup",
            "RUSTUP_HOME": "/var/lib/rustup",
            "RUSTUP_DIST_SERVER": "file:///run/build/rustup/static.rust-lang.org"
        }
    },
    "build-commands": [
        "chmod +x rustup-init && ./rustup-init -y --default-toolchain 1.91.1 --profile minimal --no-modify-path",
        "ln -s /var/lib/rustup/toolchains/1.91.1-${FLATPAK_ARCH}-unknown-linux-gnu /var/lib/rustup/toolchains/stable-${FLATPAK_ARCH}-unknown-linux-gnu"
